{
  "name": "theatre-container",
  "version": "1.0.1",
  "description": "A dependency injection container for the web",
  "directories": {
    "lib": "lib"
  },
  "format": "esm",
  "registry": "jspm",
  "main": "main.ts",
  "scripts": {
    "test": "karma start --single-run",
    "postinstall": "jspm install",
    "start": "karma start"
  },
  "author": "David Jegat <david.jegat@gmail.com>",
  "license": "MIT",
  "jspm": {
    "configFile": "jspm-config.js",
    "dependencies": {
      "ts": "github:frankwallis/plugin-typescript@^3.0.3"
    },
    "devDependencies": {
      "ts-runtime": "npm:babel-runtime@^5.8.24",
      "typescript": "npm:typescript@^1.6.2"
    }
  },
  "repository": "theatre-components/theatre-container",
  "devDependencies": {
    "jasmine-core": "^2.4.1",
    "jspm": "^0.16.29",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.7",
    "karma-jspm": "^2.0.2",
    "karma-mocha-reporter": "^1.2.0",
    "karma-systemjs": "^0.11.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.7",
    "typings": "^0.6.8"
  }
}
